{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"paths":{
			"$lib":["src/lib"],
			"$lib/*":["src/lib/*"],
			"$providers":["src/providers"],
			"$providers/*":["src/providers/*"],
			"$components/*":["src/components/*"],
			
		}
	},
	"include": ["src/**/*.d.ts","src/**/*.js","src/**/*.svelte"]
}
