{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"paths":{
			"$lib":["src/config"],
			"$lib/*":["src/config/*"],
		}
	},
	"include": ["src/**/*.d.ts","src/**/*.js","src/**/*.svelte"]
}
